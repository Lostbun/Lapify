{
  "version": 2,
  "builds": [
    {
      "src": "/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/laptops.xml",
      "headers": {
        "Content-Type": "text/xml"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}